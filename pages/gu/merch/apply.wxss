.page{
  background-color: #ffffff;
}
.fui-cell{
  border:1px solid rgba(199,199,199,1);
  margin-bottom: 20rpx;
}
.fui-cell-label{
color:rgba(0,0,0,1) !important;
}
.picker>image{
  width: 31rpx;
  height: 18rpx;
}
.fui-cell-info{
  justify-content: flex-end;
}
.placeholder-class{
  font-size: 28rpx;
  color: rgba(0,0,0,1) !important;
}
.require{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(244,9,9,1);

  padding-bottom:20rpx;
  padding-left: 20rpx;

}
.region-picker{
  justify-content: center;
}
.picker-province{
  margin-right: 70rpx;
  border-bottom: 1px solid;
}
.picker-city{
  border-bottom: 1px solid;
}
/* 上传logo */
.upload-logo{
  border: 1px solid rgba(199,199,199,1);
margin-bottom: 20rpx;
flex-direction: column;
overflow: hidden;


}
.upload-logo-label{
  height:50rpx;
  line-height: 50rpx;
  background:rgba(250,250,250,1);
  width: 100%;
  padding-left: 24rpx;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);

  border-bottom: 1px solid rgba(199,199,199,1);

}
.upload-logo-background{
  width: 100%;
  height: 400rpx;
  justify-content: center;
  overflow: hidden;

  position: relative;
}
.desc-number{
  position: absolute;
  bottom: 0;
  right: 20rpx;
  color: #B7B7B7;
  font-size: 30rpx;
}
.logo-backgruond{
  width: 100%;
  height: 400rpx;
}
.upload-logo-icon{
  width: 60rpx;
  height: 60rpx;
}

.upload-logo-input{
  width: 94%;
  height: 90%;
}
.placeholder-class1{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
}

/* 同意 */
.agreen{
  margin-top: 256rpx;
  justify-content: flex-start;
}
.agreen>.checkbox{
  width:25rpx;
  height:25rpx;
  background:rgba(255,255,255,1);
  border:1px solid rgba(175,175,175,1);
  justify-content: center;
  margin-right: 8rpx;
}
.agreen-text{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
}
.new-btn{
  margin-top: 90rpx;
  background:rgba(244,106,38,1) !important;
  border: none !important;
  margin-bottom: 100rpx;
}

/*图片样式*/
.content{
  padding: 40rpx 0;

  justify-content: center;
  flex-direction: column;
}
.content-image{
  width:276rpx;
  height:276rpx;
}