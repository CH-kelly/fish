<template name='tpl_list'><view class='fui-goods-item'><navigator hoverClass='none' openType='navigate' url='{{(item.bargain > 0? ("../../../bargain/detail/detail?id=")  +  (item.bargain) : ("/pages/goods/detail/index?id=")  +  (item.id) )}}'><view class='image' style='background-image:url({{item.thumb}})'></view><block wx:if='{{ ( (item.saleout && item.cansee <= 0)  ||  ( (item.saleout && item.cansee > 0)  && item.seecommission <= 0) ) }}'><image class='salez' src='{{item.saleout}}'></image></block><block wx:if='{{ (item.cansee > 0 && item.seecommission > 0) }}'><view class='goods-Commission'>{{item.seetitle}}￥{{item.seecommission}}</view></block></navigator><view class='detail goods_list_detail'><navigator hoverClass='none' openType='navigate' url='{{(item.bargain > 0? ("../../../bargain/detail/detail?id=")  +  (item.bargain) : ("/pages/goods/detail/index?id=")  +  (item.id) )}}'><view class='name'><block wx:if='{{item.type == 9}}'><text class='cycle-tip'>周期购</text></block><block wx:if='{{item.bargain > 0}}'><text class='cycle-tip'>砍价</text></block><text>{{item.title}}</text></view></navigator><view class='original_price'><block wx:if='{{item.productprice > 0}}'><text>¥{{item.productprice}}</text></block></view><view class='price'><view class='text'>¥{{item.minprice}}</view><block wx:if='{{ ( (item.type != 9 && item.type != 5)  && item.bargain == 0) }}'><view bindtap='selectPicker' data-buytype='buy' data-home='1' data-id='{{item.id}}' hidden='{{item.total <= 0}}'><text class='buy icox icox-gouwuche buybtnbtn buybtn-5' style='color:#ff5555'></text></view></block><block wx:if='{{ (item.type == 9 && item.bargain == 0) }}'><navigator class='buy buybtnbtn buybtn-1 ' style='' url='/pages/goods/detail/index?id={{item.id}}'>详情</navigator></block><block wx:if='{{ (item.type == 5 && item.bargain == 0) }}'><navigator class='buy buybtnbtn buybtn-1' style='' url='/pages/goods/detail/index?id={{item.id}}'>详情</navigator></block><block wx:if='{{item.bargain > 0}}'><navigator class='buy buybtnbtn buybtn-1' style='' url='../../../bargain/detail/detail?id={{item.bargain}}'>详情</navigator></block></view></view></view></template><block wx:if='{{modelShow}}'><view class='model'><view class='inner1'><view class='title'>用户未授权</view><view class='image'><image src='{{ (imgUrl)  +  ("plugin/app/static/images/wxapp/shouquan.png") }}'></image></view><view class='subtitle'>您需要先开启【用户信息】，才能使用此功能哦~</view><view class='model-btns'><view bindtap='cancelclick' class='cancel'>取消</view><view bindtap='confirmclick' class='confirm'>去设置</view></view></view></view></block><view class='page header-sort navbar goodslist {{(quick?"paddingTop":"")}}'><view class='fui-header'><view class='searchbar'><icon size='18' type='search'></icon><input bindconfirm='bindSearch' bindfocus='bindFocus' bindinput='bindInput' focus='{{focusin}}' name='search' placeholder='输入关键字进行搜索' value='{{params.keywords}}'></input></view><block wx:if='{{!(fromsearch)}}'><text bindtap='changeMode' class='icon icox {{(listmode == ""?"icox-app":"icox-viewlist")}}'></text></block><block wx:if='{{fromsearch}}'><view bindtap='bindback' class='cancel'>取消</view></block></view><view class='fui-goods-group {{listmode}}'><block wx:for='{{list}}' wx:for-item='item' wx:for-index='index' wx:key='{{index}}'><template is='tpl_list' data='{{item:item}}'></template></block></view><block wx:if='{{loading}}'><view class='fui-loading'><view class='icon'></view><view class='text'>正在加载</view></view></block><block wx:if='{{ (list.length == total && total > 0) }}'><view class='fui-loading empty'><view class='text'>没有更多了</view></view></block><block wx:if='{{ (total <= 0 && !(loading)) }}'><view class='fui-loading empty'><view class='text'>暂时没有任何商品</view></view></block><block wx:if='{{fromsearch}}'><view class='records'><block wx:if='{{searchRecords.length > 0}}'><view class='records-title'><view bindtap='delRecord' class='pull-right text-danger'>清除</view><view>历史搜索</view></view><view class='navs'><block wx:for='{{searchRecords}}' wx:for-item='item' wx:for-index='index' wx:key='{{index}}'><nav bindtap='bindnav' data-text='{{item}}'>{{item}}</nav></block></view></block><view hidden='{{true}}' class='records-title'><view class='pull-right text-danger'>换一批</view><view>热门搜索</view></view><view hidden='{{true}}' class='navs'><nav bindtap='bindnav' data-text='推子'>推子</nav><nav bindtap='bindnav' data-text='推子'>推子</nav><nav bindtap='bindnav' data-text='推子'>推子</nav></view></view></block><include src='/pages/common/goods-picker.wxml'></include><include src='/pages/common/menu.wxml'></include><include src='/pages/common/city-picker.wxml'></include></view><view class='fui-toast {{(FoxUIToast.show?"in":"out")}}'><view class='text'>{{FoxUIToast.text}}</view></view><view bindtap='emptyActive' class='nav-mask' style='{{(active?"display:block":"")}}'></view><view bindtap='showFilter' class='fui-mask {{(isFilterShow?"show":"hide")}}'></view>